<xlue>

<objtreetemplate id="HelloBolt.Tree" class="ObjectTreeTemplate">
	<attr>
			<left>-200</left>
			<top>-200</top>
			<width>2000</width>
			<height>2000</height>
	</attr>

	<obj id="root" class="LayoutObject">
		<attr>
			<left>0</left>
			<top>0</top>
			<width>429</width>
			<height>267</height>
		</attr>
		<children>
			<obj id="mainwnd.bkg1" class="Thunder.Form">
					<attr>
							<width>father.width</width>
							<height>father.height</height>
						</attr>
				<children>	
					<obj id="mainwnd.body" class="LayoutObject">
						<attr>
							<left>0</left>
							<top>30</top>
							<width>father.width</width>
							<height>father.height-30</height>
						</attr>	
						<children>			
							
						<obj id="reload" class="Thunder.ImageButton">
									<attr>
										<left>100</left>
										<top>100</top>
										<width>17</width>
										<height>17</height>
										<halign>center</halign>
										<NormalBkg>bitmap.reload.normal</NormalBkg>
										<HoverBkg>bitmap.reload.hover</HoverBkg>
										<DownBkg>bitmap.reload.down</DownBkg>
										<TipText>tiptiptiptiptip</TipText>
									</attr>
									<eventlist>
								
									</eventlist>
								</obj>
								
							<obj id="startcheck" class="Thunder.ImageButton">
									<attr>
										<left>100</left>
										<top>118</top>
										<width>17</width>
										<height>17</height>
										<halign>center</halign>
										<NormalBkg>bitmap.stop.normal</NormalBkg>
										<HoverBkg>bitmap.stop.hover</HoverBkg>
										<DownBkg>bitmap.stop.down</DownBkg>
									</attr>
									<eventlist>
					
									</eventlist>
								</obj>
								
							<obj id="leakrepair" class="Thunder.ImageButton">
									<attr>
										<left>100</left>
										<top>138</top>
										<width>17</width>
										<height>17</height>
										<halign>center</halign>
										<NormalBkg>bitmap.add.normal</NormalBkg>
										<HoverBkg>bitmap.add.hover</HoverBkg>
										<DownBkg>bitmap.add.down</DownBkg>
									</attr>
									<eventlist>
										<event name="OnLButtonDown" file="MainWnd.xml.lua" func="OnClose" />
									</eventlist>
								</obj>																
																											
						</children>							
					</obj>					
				</children>										
			</obj>
		</children>
	</obj>
</objtreetemplate>
	
<hostwndtemplate id="HelloBolt.Wnd" class="FrameHostWnd">
	<attr> 
		<title>BoltFox</title>
		<layered>1</layered>
		<left>200</left> 
		<top>100</top>
		<width >800</width>
		<height>600</height>
		<cacheleft>0</cacheleft> 
		<cachetop>0</cachetop>
		<cachewidth >1180</cachewidth>
		<cacheheight>768</cacheheight>
		<center>1</center>
		<topmost>0</topmost>
		<visible>1</visible>
		<enable>1</enable>
		<maxbox>1</maxbox>
		<minbox>1</minbox>
		<sysmenu>1</sysmenu>
		<minwidth>800</minwidth>
		<minheight>600</minheight>
		<appwindow>1</appwindow>
		<fps>30</fps>
		<ellipse>5,5</ellipse>
	</attr>
	<eventlist>
		<event name="OnSize" file="MainWnd.xml.lua" func="OnMainWndSize"/>
	</eventlist>
</hostwndtemplate>
